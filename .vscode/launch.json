{
  "version": "0.2.0",
  "configurations": [
    {
      "name":"Python Debugger: Current File",
      "type":"debugpy",
      "request":"launch",
      "program":"${file}",
      "console":"integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Anonymizer GUI",
      "type": "debugpy",
      "request": "launch",
      "module": "anonymizer.anonymizer",
      "cwd": "${workspaceFolder}/src",
      "env": {
        "PYTHONOPTIMIZE": "0",
      },
      "console": "integratedTerminal",
      "autoReload": {
        "enable": true
      },
      "justMyCode": true,
    },
  ]
}
