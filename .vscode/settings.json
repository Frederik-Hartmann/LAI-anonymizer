{
  "workbench.editor.labelFormat": "medium", // short,medium,long
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true
  },
  "cSpell.enableFiletypes": [
    "!json",
    "!jsonc",
    "!jupyter",
    "!markdown",
    "!plaintext",
    "!python",
    "!script",
    "!yaml"
  ],
  "python.testing.pytestArgs": [
    "tests"
  ],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "cSpell.words": [
    "DICOM",
    "RSNA",
    "uids"
  ],
  "python.analysis.typeCheckingMode": "standard",
  "editor.defaultFoldingRangeProvider": "ms-python.black-formatter",
  "editor.defaultFormatter": "ms-python.black-formatter",
  "python.analysis.inlayHints.functionReturnTypes": false,
  "python.analysis.inlayHints.variableTypes": false,
  "python.analysis.inlayHints.pytestParameters": false,
  "python.analysis.inlayHints.callArgumentNames": "all",
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.autoFormatStrings": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.ignore": [
    "builtins.pyi"
  ],
  "python.analysis.exclude": [
    ".vscode",
    "prototyping"
  ],
  "python.analysis.addImport.heuristics": false,
}
